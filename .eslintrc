{
  "extends":"openlayers",
  "globals":{
    "BasiGX":false,
    "Ext":false,
    "GeoExt":false,
    "ol":false,
    "Jsonix": false,
    "SLD_1_0_0": false,
    "Filter_1_0_0": false,
    "GML_2_1_2": false,
    "XLink_1_0": false,
    "opentype": false,
    "shpwrite": false,
    "Uint8Array": false
  },
  "rules":{
    "consistent-this":[
      1,
      "me"
    ],
    "indent":[
      1,
      4,
      {
        "VariableDeclarator":1,
        "SwitchCase":1
      }
    ],
    "max-len":[
      2,
      80,
      4
    ],
    "one-var":[
      2,
      "never"
    ],
    "quote-props":[
      1,
      "consistent"
    ],
    "valid-jsdoc":[
      2,
      {
        "prefer":{
          "returns":"return"
        },
        "requireReturn":false
      }
    ]
  }
}
