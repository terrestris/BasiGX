{
  "extends":"openlayers",
  "globals":{
    "afterEach":false,
    "after":false,
    "beforeEach":false,
    "describe":false,
    "document":false,
    "expect":false,
    "it":false,
    "sinon":false,
    "Ext":false,
    "GeoExt":false,
    "ol":false,
    "TestUtil":false
  },
  "rules":{
    "consistent-this":[
      1,
      "me"
    ],
    "indent":[
      1,
      4,
      {
        "VariableDeclarator":1,
        "SwitchCase":1
      }
    ],
    "max-len":[
      2,
      80,
      4
    ],
    "one-var":[
      2,
      "never"
    ],
    "quote-props":[
      1,
      "consistent"
    ],
    "valid-jsdoc":[
      2,
      {
        "prefer":{
          "returns":"return"
        },
        "requireReturn":false
      }
    ]
  }
}
